{
  "name": "copy-branch-name",
  "displayName": "Copy Branch Name",
  "description": "Extension to copy the current branch name",
  "publisher": "PKief",
  "author": {
    "name": "Philipp Kief",
    "email": "philipp.kief@gmx.de",
    "url": "https://pkief.github.io"
  },
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.69.0"
  },
  "keywords": [
    "git",
    "branch"
  ],
  "icon": "logo.png",
  "galleryBanner": {
    "color": "#1e1e1e",
    "theme": "dark"
  },
  "funding": "https://github.com/sponsors/PKief",
  "sponsor": {
    "url": "https://github.com/sponsors/PKief"
  },
  "homepage": "https://github.com/PKief/vscode-extension-copy-branch-name/blob/main/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/PKief/vscode-extension-copy-branch-name.git"
  },
  "bugs": {
    "url": "https://github.com/PKief/vscode-extension-copy-branch-name/issues"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "copy-branch-name.copy-current",
        "title": "Copy current branch name"
      }
    ]
  },
  "extensionDependencies": [
    "vscode.git"
  ],
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.69.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "prettier": "^2.7.1",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "copy-to-clipboard": "^3.3.1"
  }
}
